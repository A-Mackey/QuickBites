{
  "name": "vue-notus",
  "homepage": "https://demos.creative-tim.com/vue-notus/",
  "version": "1.0.0",
  "description": "Vue Notus - A free Tailwind CSS and VueJS UI Kit and Admin by Creative Tim.",
  "repository": "https://github.com/creativetimofficial/vue-notus",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --respawn --transpile-only ./server.ts",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && gulp licenses",
    "lint": "vue-cli-service lint",
    "build:tailwind": "tailwind build src/assets/styles/index.css -o src/assets/styles/tailwind.css",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm run serve"
  },
  "dependencies": {
    "@azure/cosmos": "^2.1.5",
    "@fortawesome/fontawesome-free": "5.14.0",
    "@popperjs/core": "2.5.1",
    "@tailwindcss/custom-forms": "0.2.1",
    "@types/express": "^4.17.0",
    "@types/node": "^12.0.8",
    "async": "^3.0.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "chart.js": "2.9.3",
    "chunk": "^0.0.3",
    "core-js": "3.6.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase": "^8.2.9",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "tailwindcss": "1.8.10",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.1",
    "vue": "2.6.12",
    "vue-axios": "^3.2.4",
    "vue-router": "^3.4.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@vue/cli-plugin-babel": "4.5.6",
    "@vue/cli-plugin-eslint": "4.5.6",
    "@vue/cli-service": "4.5.6",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-vue": "6.2.2",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
